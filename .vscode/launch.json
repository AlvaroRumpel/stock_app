    "version": "0.2.0",
    "configurations": [

        {
            "name": "Dev Debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "dev",
                "--dart-define=REMOTE_URL=",
                "--dart-define=REMOTE_KEY="
            ],
        },
        {
            "name": "stock_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "stock_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}